myPets = ['Gabby', 'Maria', 'Picasso', 'Nancy', 'Maenad', 'Jack', 'Waldo', 'Slugo']
print('Enter a pet name:')
name = input()
if name not in myPets:
    print('I do not have a pet named ' + name)
else:
    print(name + ' is my pet.')




cat = ['hippocatamus', 'Manx', 'Scary']
size, breed, threat_level = cat

