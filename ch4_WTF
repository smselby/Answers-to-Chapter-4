name = 'Ted ~ Blue ~ William ~ Jordan ~ '
for i in name:
    print('* ~ * ~ ' + i + ' ~ * ~ *')
for each in range(2):
    name = 'We ~ have ~ no ~ idea! ~ '
for i in name:
    print('* * * ' + i + ' * * *')
